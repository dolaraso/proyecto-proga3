<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.cybersentinels.vista.MainWindow">
  <grid id="mainGrid" row-count="5" column-count="2" layout-manager="GridLayoutManager">
    <margin top="20" left="20" bottom="20" right="20"/>
    <constraints>
      <xy x="20" y="20" width="800" height="600"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <component id="1" class="javax.swing.JLabel" binding="lblBienvenida">
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="2" vsize-policy="0" hsize-policy="3" anchor="0" fill="0" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text value="Bienvenido al Sistema ShieldLink Manager"/>
        </properties>
      </component>
      <component id="2" class="javax.swing.JButton" binding="btnUsuarios">
        <constraints>
          <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text value="Gestión de Usuarios"/>
        </properties>
      </component>
      <component id="3" class="javax.swing.JButton" binding="btnHerramientas">
        <constraints>
          <grid row="1" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text value="Gestión de Herramientas"/>
        </properties>
      </component>
      <component id="4" class="javax.swing.JButton" binding="btnPrestamos">
        <constraints>
          <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text value="Gestión de Préstamos y Reportes"/>
        </properties>
      </component>
      <component id="5" class="javax.swing.JButton" binding="btnMantenimiento">
        <constraints>
          <grid row="2" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text value="Gestión de Mantenimiento"/>
        </properties>
      </component>
      <component id="6" class="javax.swing.JButton" binding="btnSalir">
        <constraints>
          <grid row="3" column="0" row-span="1" col-span="2" vsize-policy="0" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text value="Cerrar Aplicación"/>
        </properties>
      </component>
    </children>
  </grid>
</form>
