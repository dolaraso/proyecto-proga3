<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.cybersentinels.vista.HerramientasWindow">
  <grid id="27dc6" binding="panelHerramientas" default-binding="true" layout-manager="GridLayoutManager" row-count="3" column-count="3" same-size-horizontally="false" same-size-vertically="false" hgap="10" vgap="10">
    <margin top="10" left="10" bottom="10" right="10"/>
    <constraints>
      <xy x="20" y="20" width="600" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <!-- Etiqueta para el título -->
      <component id="de1b2" class="javax.swing.JLabel">
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="3" vsize-policy="0" hsize-policy="0" anchor="0" fill="0" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text value="Gestión de Herramientas"/>
        </properties>
      </component>
      <!-- Tabla de herramientas -->
      <component id="1e48b" class="javax.swing.JTable" binding="tablaHerramientas">
        <constraints>
          <grid row="1" column="0" row-span="1" col-span="3" vsize-policy="6" hsize-policy="6" anchor="0" fill="3" indent="0" use-parent-layout="false">
            <preferred-size width="450" height="200"/>
          </grid>
        </constraints>
        <properties/>
      </component>
      <!-- Botón Agregar -->
      <component id="a8fc4" class="javax.swing.JButton" binding="btnAgregar">
        <constraints>
          <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text value="Agregar"/>
        </properties>
      </component>
      <!-- Botón Editar -->
      <component id="76c67" class="javax.swing.JButton" binding="btnEditar">
        <constraints>
          <grid row="2" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text value="Editar"/>
        </properties>
      </component>
      <!-- Botón Eliminar -->
      <component id="6f227" class="javax.swing.JButton" binding="btnEliminar">
        <constraints>
          <grid row="2" column="2" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text value="Eliminar"/>
        </properties>
      </component>
    </children>
  </grid>
</form>
