<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.cybersentinels.vista.MenuAdministrador">
  <grid id="adminGrid" row-count="6" column-count="1" layout-manager="GridLayoutManager">
    <margin top="20" left="20" bottom="20" right="20"/>
    <constraints>
      <xy x="20" y="20" width="600" height="500"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <component id="1" class="javax.swing.JLabel">
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="0" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text value="Menú Principal - Administrador"/>
        </properties>
      </component>
      <component id="2" class="javax.swing.JButton" binding="btnGestionUsuarios">
        <constraints>
          <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text value="Gestión de Usuarios"/>
        </properties>
      </component>
      <component id="3" class="javax.swing.JButton" binding="btnGestionHerramientas">
        <constraints>
          <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text value="Gestión de Herramientas"/>
        </properties>
      </component>
      <component id="4" class="javax.swing.JButton" binding="btnGestionPrestamos">
        <constraints>
          <grid row="3" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text value="Gestión de Préstamos"/>
        </properties>
      </component>
      <component id="5" class="javax.swing.JButton" binding="btnGestionReportes">
        <constraints>
          <grid row="4" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text value="Generar Reportes"/>
        </properties>
      </component>
      <component id="6" class="javax.swing.JButton" binding="btnSalir">
        <constraints>
          <grid row="5" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="3" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text value="Cerrar Sesión"/>
        </properties>
      </component>
    </children>
  </grid>
</form>
